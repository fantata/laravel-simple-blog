{
    "name": "fantata/laravel-simple-blog",
    "description": "Simple Laravel Blog",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fantata\\LaravelSimpleBlog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fantata\\LaravelSimpleBlog\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Chris Read",
            "email": "chris@fantata.com"
        }
    ],
    "extra": {
